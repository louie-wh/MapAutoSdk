<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment.TeamUpFragment">

    <RelativeLayout
        android:layout_width="@dimen/dp_762"
        android:layout_height="match_parent"
        android:background="@drawable/more_bg"
        android:layout_marginTop="@dimen/dp_24"
        android:layout_marginLeft="@dimen/dp_40"
        android:layout_marginBottom="@dimen/dp_24">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_20">

            <Button
                android:id="@+id/btn_team_up_my_team"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="查询队伍"
                android:textColor="@color/white"
                android:background="@color/black"/>

            <Button
                android:id="@+id/btn_team_up_create_team"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="创建队伍"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <Button
                android:id="@+id/btn_team_up_del_team"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="解散队伍"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <Button
                android:id="@+id/btn_team_up_enter_team"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="加入队伍"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <Button
                android:id="@+id/btn_team_up_quit_team"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="退出队伍"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <Button
                android:id="@+id/btn_team_up_kick_team"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="踢出队伍"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <Button
                android:id="@+id/btn_team_up_get_group_info"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="查询队伍信息"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <Button
                android:id="@+id/btn_team_up_update_group_info"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="修改队伍信息"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <Button
                android:id="@+id/btn_team_up_update_nickname"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="修改本人昵称"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginLeft="@dimen/dp_300">

            <Button
                android:id="@+id/btn_team_up_loc"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:text="上报位置"
                android:textColor="@color/white"
                android:background="@color/black"/>

            <TextView
                android:id="@+id/tv_list"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <TextView
                android:id="@+id/tv_list_two"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

            <TextView
                android:id="@+id/tv_list_three"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_50"
                android:textColor="@color/white"
                android:background="@color/black"
                android:layout_marginTop="@dimen/dp_20"/>

        </LinearLayout>

    </RelativeLayout>

</RelativeLayout>